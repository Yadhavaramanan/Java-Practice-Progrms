import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		int s = sc.nextInt();
		int[] arr = new int[s];
		for(int i = 0; i<s; i++ ){
		    arr[i] = sc.nextInt();
		}
		Arrays.sort(arr);
		Main m = new Main();
	    ArrayList<Integer> list =m.TwoPointer(arr, T);
	
		System.out.println(list);
	}
		public ArrayList<Integer> TwoPointer(int[] arr, int T){
		    int left =0;
		    int right = arr.length-1;
		    ArrayList<Integer> list=new ArrayList<>();
		    while(left<right){
		       int currentSum = arr[left]+arr[right];
		       
		       if(currentSum == T){
		           list.add(left);
		           list.add(right);
		          break;
		       }else if(currentSum < T){
		           left++;
		    }else{
		        right--;;
		    }
		    }
		    return list;
		}
	
}
